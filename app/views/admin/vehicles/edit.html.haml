= render "photo_upload"
.row
  = form_for [:admin, vehicle] do |form|
    .col-md-8
      .card
        .header
          %h4.title Edit Vehicle Information
        .content
          %form
            = render 'form', form: form
          .clearfix
            = form.submit 'Update Vehicle', class: 'btn btn-info btn-fill pull-right'
